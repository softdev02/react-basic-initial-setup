{
  "name": "ascentware_hr_management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "cross-env VITE_USE_MOCK=true vite",
    "build": "tsc -b && vite build",
    "build:mock": "cross-env VITE_USE_MOCK=true tsc -b && vite build",
    "preview": "vite preview",
    "preview:mock": "cross-env VITE_USE_MOCK=true vite preview",
    "lint": "npm run test:format && npm run test:types && npm run test:lint",
    "test": "npm run test:format && npm run test:types && npm run test:lint && npm run test:unit",
    "test:types": "tsc --build --noEmit",
    "test:unit": "vitest run",
    "test:format": "npm run format:check",
    "test:lint": "eslint --format=codeframe --max-warnings=0 ./",
    "vitest:watch": "vitest watch --test-timeout 0",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx}",
    "serve": "vite preview --port 3000",
    "cypress:run": "npm run build &&  cypress run",
    "cypress:open": "npm run build && cypress open"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-query": "^5.69.0",
    "axios": "^1.8.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "tailwindcss": "^4.0.14",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.2.0",
    "eslint": "^9.22.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-mock-dev-server": "^1.8.4",
    "vitest": "^3.0.9"
  }
}
